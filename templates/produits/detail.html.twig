	<div class="Accueil-produit">
				<div class="card-detail">
					<div class="content">
						<h1 class="mt-6">{{detail.nom}}</h1>
						<br>
						<p class="mt-2">{{detail.description}}</p>
						<p>Réference : {{detail.Numero.ReferenceNumero}}</p>
						<p>Catégorie : {{detail.categorie.NomCategorie}}</p>
						{% for details in detail  %}
						<p>Distributeur : {{details.nom_distributeur.NomDistributeur}}</p>
						{% endfor %}
						<br>
						<p>Prix :
							{{detail.prix}}
							€<span class="ml-6">Disponible le :<time datetime="2016-1-1" class="has-text-wite">{{ detail.disponible ? detail.disponible|date ('Y-m-d') : '' }}</time>
							</span>
						</p>
						{% if (app.user) %}
							<div class="card-footer">
							<button class="a-btn-2" style="background: rgba(255, 255, 255, 0.1);"><a href="{{path('app_produit_edit',{'id':detail.id})}}">Éditer le produit</a></button>
							{{include('produits/delete_form.html.twig')}}
							</div>

						{% endif %}
					</div>
				</div>

				<div class="card-detail-image">
					<div class="content">
						<div class="picture">
							<p class="has-text-black has-text-centered">Borne Arcade</p>
