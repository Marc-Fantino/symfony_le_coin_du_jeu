								<img src="/image/{{detail.photo}}" alt="">
								<div class="icon-arcade">
									<i class="fa fa-arrows mr-2" aria-hidden="true"></i>
									<i class="fa fa-circle icon-1 mr-2" aria-hidden="true"></i>
									<i class="fa fa-circle icon-2 mr-2" aria-hidden="true"></i>
									<i class="fa fa-circle icon-3 mr-2" aria-hidden="true"></i>
									<hr>
								</div>

							</div>
						</div>
